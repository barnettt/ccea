		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public abstract T findById(Integer id);
	

	@Override
	public abstract T findByName(String name);
	
	@Override
	public List<T> findByObject(Object obj) {
		// TODO Auto-generated method stub
		return null;
	}
	
	

}
