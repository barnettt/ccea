package com.ccea.persistence;

import java.util.Map;
import java.util.List;
import java.util.Date;

import com.ccea.persistence.model.pojo.Project;

/**
 * Report service interface
 * 
 * @author abuayyub
 *
 * @param <I>
 * @param <T>
 */
public interface ReportService<I, T> {

	String getMessage();

	public Map<I, List<T>> getAllProjectReport();

	public Map<I, List<T>> getProjectReport(Project project);
	
	public Map<I, List<T>> getReportAllProjectBetweenDates(Date startDate,Date endDate);

}
