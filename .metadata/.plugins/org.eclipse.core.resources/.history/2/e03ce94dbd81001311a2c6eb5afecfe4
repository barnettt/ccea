package com.ccea.web.beans;

import java.util.List;

import com.ccea.persistence.model.pojo.Project;
import javax.faces.model.SelectItem;

public class ProjectBeanWrapper {

	
	List<SelectItem> projects;
	List<Project> originalProjects;
	
	
	public ProjectBeanWrapper() {
		// TODO Auto-generated constructor stub
	}

	/**
	 * @return the projects
	 */
	public List<Project> getOriginalProjects() {
		return originalProjects;
	}

	/**
	 * @param projects the projects to set
	 */
	public void setOriginalProjects(List<Project> projects) {
		this.originalProjects = projects;
	}

	
}
