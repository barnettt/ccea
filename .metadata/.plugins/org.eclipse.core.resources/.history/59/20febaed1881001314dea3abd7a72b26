package com.ccea.web.action;

import org.springframework.webflow.execution.Action;
import org.springframework.webflow.execution.Event;
import org.springframework.webflow.execution.RequestContext;

public class EnterProjectAction implements MultiAction {

	public EnterProjectAction() {
		 super();
	}

	@Override
	public Event execute(RequestContext requestContext) throws Exception {
		Project project = new Project();
		requestContext.getFlowScope().put("projectBean", project);
		
		return new Event();
	}

}
